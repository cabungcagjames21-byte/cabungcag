import java.util.Scanner;

public class cabungcag17
{
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int[] numbers = new int[10];

        System.out.println("Enter 10 integers:");

        for (int i = 0; i < numbers.length; i++) {
            System.out.print("Number " + (i + 1) + ": ");
            numbers[i] = scanner.nextInt();
        }

        int largestOdd = Integer.MIN_VALUE;
        boolean foundOdd = false;

        for (int num : numbers) {
            if (num % 2 != 0) { // Check if odd
                if (!foundOdd || num > largestOdd) {
                    largestOdd = num;
                    foundOdd = true;
                }
            }
        }

        if (foundOdd) {
            System.out.println("The largest odd number is: " + largestOdd);
        } else {
            System.out.println("No odd numbers found in the array.");
        }

        scanner.close();
    }
}
