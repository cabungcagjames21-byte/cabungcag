public class james3
{
    public static void main(String[] args) {
        // Loop for each line from 1 to 5
        for (int i = 1; i <= 5; i++) {
            // Print 'i' stars in the current line
            for (int j = 1; j <= i; j++) {
                System.out.print("*");
            }
            // Move to next line after printing stars
            System.out.println();
        }}
}
